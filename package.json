{
  "name": "@scintilla-network/keys",
  "version": "1.0.0-beta.19",
  "description": "Keys for Scintilla",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",  
    "test": "litest",
    "test:accountkeyring": "litest src/keyrings/AccountKeyring/AccountKeyring.spec.js",
    "test:e2e": "litest --watch test/e2e/functional.spec.ts",
    "test:chain": "litest --watch src/ChainKeyring/ChainKeyring.spec.js",
    "test:seed": "litest --watch src/SeedKeyring/SeedKeyring.spec.js",
    "test:persona": "litest --watch src/PersonaKeyring/PersonaKeyring.spec.js",
    "test:sharedpersona": "litest --watch src/SharedPersonaKeyring/SharedPersonaKeyring.spec.js",
    "test:extendedkey": "litest --watch src/primitives/ExtendedKey/ExtendedKey.spec.js",
    "test:extendedprivkey": "litest --watch src/primitives/ExtendedPrivateKey/ExtendedPrivateKey.spec.js",
    "test:extendedpubkey": "litest --watch src/primitives/ExtendedPublicKey/ExtendedPublicKey.spec.js",
    "test:privatekey": "litest --watch src/primitives/PrivateKey/PrivateKey.spec.js",
    "test:publickey": "litest src/primitives/PublicKey/PublicKey.spec.js",
    "test:address": "litest --watch src/primitives/Address/Address.spec.js"
  },
  "repository": {
      "type": "git",
      "url": "git+https://github.com/Scintilla-Network/keys.git"
  },
  "homepage": "https://github.com/Scintilla-Network/keys",
  "bugs": {
      "url": "https://github.com/Scintilla-Network/keys/issues"
  },
  "keywords": [
    "scintilla",
    "keys",
    "keyrings",
    "mnemonic",
    "bip39",
    "derivation",
    "address",
    "privatekey",
    "publickey",
    "extendedkey",
    "extendedprivatekey",
    "extendedpublickey",
    "persona",
    "sharedpersona",
    "chain",
    "account",
    "addresskeyring",
    "personaking",
    "sharedpersonaking"
  ],
  "type": "module",
  "author": "Scintilla Labs",
  "contributors": [
    "Alex Werner <alex@scintilla.network>"
  ],
  "license": "MIT",
  "dependencies": {
    "@scintilla-network/ciphers": "1.2.1",
    "@scintilla-network/hashes": "1.3.1",
    "@scintilla-network/mnemonic": "2.0.0",
    "@scintilla-network/signatures": "1.2.1"
  },
  "devDependencies": {
    "@scintilla-network/litest": "1.3.0"
  }
}
